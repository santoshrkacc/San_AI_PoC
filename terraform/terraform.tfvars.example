# Example Terraform variables file
# Copy this to terraform.tfvars and update with your values

environment = "dev"
project_name = "genai-etl"
location = "East US"
resource_group_name = "rg-genai-etl-dev"

# Databricks configuration
databricks_sku = "standard"
cluster_node_type = "Standard_DS3_v2"
cluster_min_workers = 1
cluster_max_workers = 3
cluster_autotermination_minutes = 60
spark_version = "13.3.x-scala2.12"
python_version = "3"

# Network configuration
enable_public_ip = true
allowed_ip_ranges = [
  "203.0.113.0/24",  # Replace with your IP range
  "198.51.100.0/24"  # Replace with your IP range
]

# Storage configuration
storage_account_tier = "Standard"
storage_replication_type = "LRS"

# Additional tags
tags = {
  Owner       = "Data Engineering Team"
  CostCenter  = "IT"
  Project     = "GenAI ETL"
}



